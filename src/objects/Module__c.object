    <validationRules>
        <fullName>ApprovalGradeRange</fullName>
        <active>true</active>
        <description>Approval grade must be a value between 0 and 100.</description>
        <errorConditionFormula>OR(ApprovalGrade__c &lt; 0, ApprovalGrade__c &gt; 1)</errorConditionFormula>
        <errorDisplayField>ApprovalGrade__c</errorDisplayField>
        <errorMessage>The approval grade must be a value between 0 and 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AttemptsAllowedPerQuestionRange</fullName>
        <active>true</active>
        <description>The field Attempts Allowed per Question must be a number that can be null or greater than 0.</description>
        <errorConditionFormula>AND( NOT( ISBLANK(  AttemptsAllowedPerQuestion__c  ) ) ,  AttemptsAllowedPerQuestion__c   &lt;= 0)</errorConditionFormula>
        <errorDisplayField>AttemptsAllowedPerQuestion__c</errorDisplayField>
        <errorMessage>When filled, the number of attempts allowed per question must be positive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AttemptsAllowedRange</fullName>
        <active>true</active>
        <description>The field Attempts Allowed must be a number that can be null or greater than 0.</description>
        <errorConditionFormula>AND( NOT( ISBLANK( AttemptsAllowed__c ) ) , AttemptsAllowed__c  &lt;= 0)</errorConditionFormula>
        <errorDisplayField>AttemptsAllowed__c</errorDisplayField>
        <errorMessage>When filled, the number of attempts allowed must be positive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotChangeCourse</fullName>
        <active>true</active>
        <description>The course cannot be changed.</description>
        <errorConditionFormula>ISCHANGED( Course__c )</errorConditionFormula>
        <errorDisplayField>Course__c</errorDisplayField>
        <errorMessage>The course cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CourseMandatory</fullName>
        <active>true</active>
        <description>The course field is mandatory.</description>
        <errorConditionFormula>ISBLANK( Course__c )</errorConditionFormula>
        <errorDisplayField>Course__c</errorDisplayField>
        <errorMessage>The course is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GradeApprovalIfActive</fullName>
        <active>true</active>
        <description>When the module is set to active must validate that the Grade Approval is not null.</description>
        <errorConditionFormula>AND( IsActive__c , ISBLANK( ApprovalGrade__c ))</errorConditionFormula>
        <errorDisplayField>ApprovalGrade__c</errorDisplayField>
        <errorMessage>Active modules must have an approval grade.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IsActiveChangedWhenCourseIsActive</fullName>
        <active>true</active>
        <description>The module &quot;IsActive&quot; field cannot be changed if the related course is active, completed or scheduled.</description>
        <errorConditionFormula>AND( ISCHANGED( IsActive__c ) ,  OR(TEXT(Course__r.Status__c) = &apos;Active&apos;,
TEXT(Course__r.Status__c) = &apos;Scheduled&apos;,
TEXT(Course__r.Status__c) = &apos;Completed&apos;)
)</errorConditionFormula>
        <errorDisplayField>IsActive__c</errorDisplayField>
        <errorMessage>&quot;Active&quot; status cannot be changed if the related course is active, completed or scheduled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ModuleNameNotNull</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW(),  ISBLANK(Name) )</errorConditionFormula>
        <errorMessage>Module Name cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoActiveCourseOnCreation</fullName>
        <active>true</active>
        <description>A Module cannot be created with a relation to an active Course, and the Course cannot be modified afterwards.</description>
        <errorConditionFormula>AND( 
      ISNEW() ,
      OR(
        ISPICKVAL(Course__r.Status__c , 
         &apos;Active&apos;),
        ISPICKVAL(Course__r.Status__c , 
         &apos;Scheduled&apos;),
        ISPICKVAL(Course__r.Status__c , 
         &apos;Completed&apos;)
      )
   )</errorConditionFormula>
        <errorDisplayField>Course__c</errorDisplayField>
        <errorMessage>On module creation, course cannot be active, completed or scheduled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoQuestionPoolIfWeightedQuestions</fullName>
        <active>true</active>
        <description>The Module cannot have a value for Question Pool and use Weighted Questions.</description>
        <errorConditionFormula>AND( NOT(ISBLANK(QuestionPool__c)) , WeightedQuestions__c)</errorConditionFormula>
        <errorMessage>The Module cannot use a Question Pool together with Weighted Questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotActiveIfNew</fullName>
        <active>true</active>
        <description>The module cannot be created with status active, as it won&apos;t have questions yet.</description>
        <errorConditionFormula>AND( ISNEW() , IsActive__c )</errorConditionFormula>
        <errorDisplayField>IsActive__c</errorDisplayField>
        <errorMessage>New module (without questions) cannot be active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QuestioPoolPositive</fullName>
        <active>true</active>
        <description>Question Pool cannot be equal to zero.</description>
        <errorConditionFormula>QuestionPool__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>QuestionPool__c</errorDisplayField>
        <errorMessage>Question Pool must be greater than zero.</errorMessage>
    </validationRules>
</CustomObject>
