    <validationRules>
        <fullName>CourseNameNotBlank</fullName>
        <active>true</active>
        <description>Course name field needs to be complete.</description>
        <errorConditionFormula>ISBLANK( Name )|| ISNULL(Name)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You must enter the Name of the Course</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CourseProgram</fullName>
        <active>true</active>
        <description>Forces the user to choose a program in the Course object.</description>
        <errorConditionFormula>ISBLANK(Program__c)||  ISNULL(Program__c)</errorConditionFormula>
        <errorDisplayField>Program__c</errorDisplayField>
        <errorMessage>You must select a Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CourseProgramCannotEdit</fullName>
        <active>true</active>
        <description>Cannot change the program from the course.</description>
        <errorConditionFormula>ISCHANGED( Program__c )</errorConditionFormula>
        <errorDisplayField>Program__c</errorDisplayField>
        <errorMessage>Cannot change the program from the course.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CourseValidateModule</fullName>
        <active>true</active>
        <description>Course need at least one Active Module if is Active.</description>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp;
NOT(ISPICKVAL( PRIORVALUE(Status__c) , &quot;Active&quot;)) &amp;&amp;
ISPICKVAL( Status__c , &quot;Active&quot;) &amp;&amp; ActiveModules__c &lt; 1</errorConditionFormula>
        <errorDisplayField>ActiveModules__c</errorDisplayField>
        <errorMessage>To activate a course, it must have at least one active related module.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SameProgramSegment</fullName>
        <active>true</active>
        <description>The course must have a segment within the same program.</description>
        <errorConditionFormula>Segment__r.FieloPLT__Program__c  &lt;&gt;  Program__c &amp;&amp;  NOT(ISNULL(Segment__r.FieloPLT__Program__c) ||  ISBLANK(Segment__r.FieloPLT__Program__c))</errorConditionFormula>
        <errorDisplayField>Segment__c</errorDisplayField>
        <errorMessage>Segment must be on the same program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateEndDateGreaterToday</fullName>
        <active>true</active>
        <description>Start Date and End Date should be greater than Today date</description>
        <errorConditionFormula>OR(ISNEW(), NOT(ISNEW()) &amp;&amp; (ISCHANGED(StartDate__c) || ISCHANGED(EndDate__c))) &amp;&amp; 

AND(NOT(StartDate__c &gt;=  TODAY()) || AND(NOT( EndDate__c &gt;=  TODAY())))</errorConditionFormula>
        <errorDisplayField>StartDate__c</errorDisplayField>
        <errorMessage>Start Date and End Date must be equal or greater than today date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateNotGreaterThanEnd</fullName>
        <active>true</active>
        <description>Start Date cannot be greater than End Date</description>
        <errorConditionFormula>OR(ISNEW(), NOT(ISNEW()) &amp;&amp; (ISCHANGED(StartDate__c) || ISCHANGED(EndDate__c))) &amp;&amp;

AND(!ISNULL( StartDate__c ), !ISNULL( EndDate__c )) &amp;&amp;  StartDate__c  &gt;  EndDate__c</errorConditionFormula>
        <errorDisplayField>StartDate__c</errorDisplayField>
        <errorMessage>Start Date cannot be greater than End Date</errorMessage>
    </validationRules>
